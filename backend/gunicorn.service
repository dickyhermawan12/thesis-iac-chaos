[Unit]
Description=miniapp api
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/cm-intern/backend/
Environment="PATH=/home/ubuntu/cm-intern/backend/venv/bin"
ExecStart=/home/ubuntu/cm-intern/backend/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:8000

[Install]
WantedBy=multi-user.target